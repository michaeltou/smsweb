<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>云模短信平台</display-name>
  <servlet>
    <servlet-name>RegisterServlet</servlet-name>
    <servlet-class>yunmo.sms.service.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CheckUserExistServlet</servlet-name>
    <servlet-class>yunmo.sms.service.CheckUserExistServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>yunmo.sms.service.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CheckCodeServlet</servlet-name>
    <servlet-class>yunmo.sms.service.CheckCodeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CheckValidCodeServlet</servlet-name>
    <servlet-class>yunmo.sms.service.CheckValidCodeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TestMessageServlet</servlet-name>
    <servlet-class>yunmo.sms.service.TestMessageServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DownloadAPIDocServlet</servlet-name>
    <servlet-class>yunmo.sms.service.DownloadAPIDocServlet</servlet-class>
  </servlet>







  <servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/servlet/RegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CheckUserExistServlet</servlet-name>
    <url-pattern>/servlet/CheckUserExistServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/servlet/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CheckCodeServlet</servlet-name>
    <url-pattern>/servlet/CheckCodeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CheckValidCodeServlet</servlet-name>
    <url-pattern>/servlet/CheckValidCodeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TestMessageServlet</servlet-name>
    <url-pattern>/servlet/TestMessageServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DownloadAPIDocServlet</servlet-name>
    <url-pattern>/servlet/DownloadAPIDocServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>